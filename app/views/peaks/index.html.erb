<p id="notice"><%= notice %></p>

<h1>Peaks</h1>

<table>
  <thead>
    <tr>
      <th>Peaksign</th>
      <th>Peakformula</th>
      <th>Peakimage</th>
      <th>Participation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @peaks.each do |peak| %>
      <tr>
        <td><%= peak.peaksign %></td>
        <td><%= peak.peakformula %></td>
        <td><%= peak.peakimage %></td>
        <td><%= peak.participation_id %></td>
        <td><%= link_to 'Show', peak %></td>
        <td><%= link_to 'Edit', edit_peak_path(peak) %></td>
        <td><%= link_to 'Destroy', peak, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Peak', new_peak_path %>
